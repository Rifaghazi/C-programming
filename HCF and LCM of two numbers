#include <stdio.h>
int main()
{int a,b,lcm,c,d;
printf("Enter number 1");
scanf("%d",&a);
printf("Enter number 2");
scanf("%d",&b);
d=b;
c=a%b;
while (c!=0)
{a=b;
b=c;
c=a%b;}
printf("Highest common factor is %d\n",b);
if (b==1)
lcm=a*d;
else
lcm=(a*d)/b;
printf("Lowest common multiple is %d",lcm);
return 0;
}
